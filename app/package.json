  "name": "jobbler",
  "version": "0.1.16",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "html-react-parser": "^1.4.12",
    "joi-browser": "^13.4.0",
    "lodash": "^4.17.20",
    "nanoid": "^3.1.20",
    "react": "^17.0.1",
    "react-datepicker": "^4.7.0",
    "react-dom": "^17.0.1",
    "react-dropdown": "github:AgrisJ/react-dropdown",
    "react-flags-select": "^2.1.2",
    "react-icons": "^4.1.0",
    "react-mobile-datepicker": "^4.0.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^2.0.5",
    "react-scroll": "^1.8.1",
    "react-to-print": "^2.12.2",
